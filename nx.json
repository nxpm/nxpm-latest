{
  "npmScope": "nxpm-latest",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-account-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-account-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-auth-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-auth-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-auth-util": {
      "tags": ["scope:api", "type:util"]
    },
    "api-core-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-core-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-core-util": {
      "tags": ["scope:api", "type:util"]
    },
    "api-e2e": {
      "tags": []
    },
    "api-user-data-access": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-user-feature": {
      "tags": ["scope:api", "type:feature"]
    },
    "web": {
      "tags": []
    },
    "web-about-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-account-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-account-ui": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-admin-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-admin-ui": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-assets": {
      "tags": ["scope:web", "type:assets"]
    },
    "web-auth-data-access": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-auth-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-auth-ui": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-core-data-access": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-core-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-dashboard-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-e2e": {
      "tags": [],
      "implicitDependencies": ["web"]
    },
    "web-layout": {
      "tags": ["scope:web", "type:layout"]
    },
    "web-shell-feature": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-style": {
      "tags": ["scope:web", "type:style"]
    },
    "web-ui-button": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-form": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-icon": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-loader": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-page": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-page-header": {
      "tags": ["scope:web", "type:ui"]
    },
    "web-ui-sidebar-page": {
      "tags": ["scope:web", "type:ui"]
    }
  },
  "plugins": {
    "@nxpm/stack": {
      "api": {
        "project": "api"
      },
      "web": {
        "project": "web"
      }
    }
  }
}
